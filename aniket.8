#include <stdio.h>

int main() 
{
    int n, sum=0,c, value;
    printf(" how many numbers you want to add");
    scanf("%d , &n");
    printf("enter %d integer\n" , n);
    for (c=1;c<=n; c++);
{
        scanf("%d" , &value);
        sum=sum+value;
}
    printf("sum of integers = %d\n" , sum);
    

    return 0;
}
